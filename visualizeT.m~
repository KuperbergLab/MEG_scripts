function visualizeT(exp, dataType, subjList, cond1, cond2, t1, t2, pVal)

load('/autofs/cluster/kuperberg/SemPrMM/MEG/scripts/ch_names.mat')

[m,nchan] = size(ch_names);
nonSigChan = {};
sigChan = {};
nonSigCount = 0;
sigCount = 0;

nchan = 1;
for ichan = 1:nchan
    p = quickT(exp, dataType, subjList, cond1, cond2, t1, t2, ch_names{ichan});
    if p > pVal
        nonSigCount = nonSigCount+1;
        nonSigChan{nonSigCount} = ch_names{ichan};
    else 
        sigCount = sigCount + 1;
        sigChan{sigCount} = ch_names{ichan};
    end
end

nonSigChan
sigChan

blankStr = fiff_read_evoked_all(strcat('/autofs/cluster/kuperberg/SemPrMM/MEG/data/ga/ave_',dataType,'ga_',exp,'-ave.fif'));

gaStr.info

gaStr.info.bads = 
